<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
      <Version>1.0.0</Version>
      <Authors>SHA</Authors>
      <Company>多奇數位創意有限公司</Company>
      <Product>ContosoPack</Product>
      <Description>展示如何建立、發佈 NuGet 套件的練習範例專案</Description>
      <Copyright>© 多奇數位創意有限公司.所有權利保留.</Copyright>
      
      <PackageTags>miniasp ContosoPack</PackageTags>
      <PackageProjectUrl>https://github.com/coolrare/ContosoPack</PackageProjectUrl>
      
      <RepositoryUrl>https://github.com/coolrare/ContosoPack.git</RepositoryUrl>
      <RepositoryType>git</RepositoryType>
      
      <IncludeSymbols>true</IncludeSymbols>
      <IncludeSource>true</IncludeSource>
      
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.7.6" />
  </ItemGroup>

</Project>
